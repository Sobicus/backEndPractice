{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testRegex": ".e2e.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [
  "./../node_modules/"
],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  }
}
